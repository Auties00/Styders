<resources>    <style name="WhiteAppTheme" parent="Base.AppTheme.Light"/>    <style name="Base.AppTheme.Light" parent="Theme.AppCompat.Light.NoActionBar">        <item name="background">@color/white_background</item>        <item name="card">@color/white_card</item>        <item name="text_color">@color/default_text_color</item>        <item name="card_color">@color/white_card_color</item>        <item name="seek_bar_color">@color/white_seek_bar</item>        <item name="seek_bar_background">@color/white_seek_bar_background</item>        <item name="switch_color">@color/white_switch</item>        <item name="switch_background">@color/white_switch_background</item>        <item name="radio_on">@color/white_radio_background</item>        <item name="radio_off">@color/white_radio_background</item>        <item name="enabled_day">@color/white_day_enabled</item>        <item name="disabled_day">@color/white_day_disabled</item>        <item name="timer_text">@color/white_timer_text</item>        <item name="activated_tab">@color/blue_text</item>        <item name="android:windowBackground">@android:color/transparent</item>        <item name="android:windowContentOverlay">@null</item>        <item name="android:windowShowWallpaper">true</item>        <item name="android:fontFamily">@font/manrope</item>        <item name="line_color">@color/gray_background</item>    </style>    <style name="DarkAppTheme" parent="Base.AppTheme.Dark"/>    <style name="Base.AppTheme.Dark" parent="Theme.AppCompat.NoActionBar">        <item name="background">@color/black_background</item>        <item name="card">@color/black_card</item>        <item name="text_color">@color/dark_text_color</item>        <item name="card_color">@color/black_card_color</item>        <item name="seek_bar_color">@color/black_seek_bar</item>        <item name="seek_bar_background">@color/black_seek_bar_background</item>        <item name="switch_color">@color/black_switch</item>        <item name="switch_background">@color/black_switch_background</item>        <item name="radio_on">@color/black_radio_background</item>        <item name="radio_off">@color/black_radio_background</item>        <item name="enabled_day">@color/black_day_enabled</item>        <item name="disabled_day">@color/black_day_disabled</item>        <item name="timer_text">@color/black_timer_text</item>        <item name="activated_tab">@color/white</item>        <item name="android:windowBackground">@android:color/transparent</item>        <item name="android:windowContentOverlay">@null</item>        <item name="android:windowShowWallpaper">true</item>        <item name="android:fontFamily">@font/manrope</item>        <item name="line_color">@color/white</item>    </style>    <style name="GrayAppTheme" parent="Base.AppTheme.Dark.Gray"/>    <style name="Base.AppTheme.Dark.Gray" parent="Theme.AppCompat.NoActionBar">        <item name="background">@color/gray_background</item>        <item name="card">@color/gray_card</item>        <item name="text_color">@color/dark_text_color</item>        <item name="card_color">@color/gray_card_color</item>        <item name="seek_bar_color">@color/gray_seek_bar</item>        <item name="seek_bar_background">@color/gray_seek_bar_background</item>        <item name="switch_color">@color/gray_switch</item>        <item name="switch_background">@color/gray_switch_background</item>        <item name="radio_on">@color/black_radio_background</item>        <item name="radio_off">@color/black_radio_background</item>        <item name="enabled_day">@color/gray_day_enabled</item>        <item name="disabled_day">@color/gray_day_disabled</item>        <item name="timer_text">@color/gray_timer_text</item>        <item name="activated_tab">@color/white</item>        <item name="android:windowBackground">@android:color/transparent</item>        <item name="android:windowContentOverlay">@null</item>        <item name="android:windowShowWallpaper">true</item>        <item name="android:fontFamily">@font/manrope</item>        <item name="line_color">@color/white</item>    </style>    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>        <item name="android:windowBackground">@android:color/transparent</item>        <item name="android:windowContentOverlay">@null</item>        <item name="android:windowShowWallpaper">true</item>    </style>    <style name="SplashScreen" parent="Theme.AppCompat.DayNight.NoActionBar">        <item name="android:windowBackground">@drawable/splash_resource</item>    </style>    <style name="EnableSwitch" parent="Theme.AppCompat.Light">        <item name="colorControlActivated">?attr/switch_background</item>        <item name="colorSwitchThumbNormal">?attr/switch_background</item>        <item name="android:colorForeground">?attr/switch_background</item>    </style>    <style name="TabLayoutStyle" parent="Widget.Design.TabLayout">        <item name="android:textSize">10sp</item>        <item name="android:textAllCaps">false</item>    </style>    <style name="AppTheme.Picker" parent="Theme.AppCompat.Light.NoActionBar">        <item name="android:textColorPrimary">?attr/text_color</item>        <item name="colorControlNormal">@android:color/transparent</item>    </style>    <style name="CardStyle" parent="CardView.Light">        <item name="cardElevation">20dp</item>    </style>    <style name="CardStyle.Home">        <item name="android:layout_marginLeft">@dimen/screen_spacing_horizontal</item>        <item name="android:layout_marginRight">@dimen/screen_spacing_horizontal</item>        <item name="android:layout_gravity">center</item>        <item name="cardElevation">0dp</item>    </style>    <style name="ColorPickerDialogDark" parent="Theme.AppCompat.Dialog">        <item name="redColor">#FF5252</item>        <item name="greenColor">#FF5252</item>        <item name="blueColor">#536DFE</item>        <item name="neutralColor">#FFFFFF</item>    </style>    <style name="CustomBottomSheetDialog" parent="Theme.MaterialComponents.BottomSheetDialog">    </style></resources>